{
  "name": "e2e-dashboard",
  "version": "0.0.1",
  "scripts": {
    "dev": "next -p 3001",
    "build": "next build",
    "start": "next start -p 8080",
    "run-app": "pm2 start next -- start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "postinstall": "husky install && chmod +x .husky/pre-commit",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "export": "next build && next export"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts(x)?": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@casperdash/usewallet": "^0.0.1",
    "@casperdash/usewallet-core": "^0.0.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.10.14",
    "@mui/lab": "^5.0.0-alpha.106",
    "@mui/material": "^5.10.14",
    "@mui/styled-engine": "^5.10.8",
    "@mui/x-date-pickers": "^5.0.8",
    "@reduxjs/toolkit": "^1.8.6",
    "@thirdweb-dev/react": "^3.6.5",
    "@thirdweb-dev/sdk": "^3.6.5",
    "@types/js-cookie": "^3.0.2",
    "axios": "^1.1.3",
    "casper-js-sdk": "2.10.1",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "i18next": "^22.4.9",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "material-react-table": "^1.3.9",
    "material-ui-popup-state": "^4.1.0",
    "mui-one-time-password-input": "^1.0.3",
    "mui-tel-input": "^2.0.4",
    "next": "^13.1.6",
    "next-i18next": "^13.1.5",
    "next-pwa": "^5.6.0",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.4",
    "react-hook-form-mui": "^5.10.2",
    "react-i18next": "^12.1.5",
    "react-icons": "^4.6.0",
    "react-query": "^3.39.2",
    "react-redux": "^8.0.4",
    "react-toastify": "^9.0.8",
    "siwe": "^1.1.6",
    "url-join": "^5.0.0",
    "wagmi": "^0.7.10",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/jest": "^11.3.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.6.0",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "storybook-addon-next-router": "^4.0.1",
    "typescript": "4.8.4"
  },
  "license": "MIT"
}
