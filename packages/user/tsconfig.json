{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "pretty": true,
    "incremental": true,
    "paths": {
      "@mlem-user/*": [
        "./src/*"
      ],
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "exclude": [
    "**/node_modules/**",
    "**/dist/**"
  ],
  "include": [
    "next-env.d.ts",
    "./src/**/*",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "extends": "../../tsconfig.json"
}
